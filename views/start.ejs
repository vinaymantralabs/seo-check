</head>

<body data-spy="scroll" data-target="#scroll-menu" data-offset="50" id="top">

<div class="main-content">

    <div class="bg-primary-color page-block">
        <div class="container">
            <h1 class="pageTitle text-center"><%= url %></h1>
        </div>
    </div>
</div><script>
    var passScore = '<%= value.finalScore %>';
    var improveScore = '<%= (70 - value.finalScore) > 0 ? (70 - value.finalScore) : 0%>';
    var errorScore = '<%= (100 - value.finalScore) %>';
</script>
<script>
    var hashCode = '4175bfbe88de00299499079bbc058c4b';
    var inputHost = '<%= url %>';
    var isOnline = '0';
    var pdfUrl = '<%= url %>';
    var pdfMsg = 'This feature is not working right now';
    var axPath = '<%= url %>';
    var scoreTxt = 'Score';
    var CANV_GAUGE_FONTS_PATH = '<%= url %>';
</script>

<script src="../default/js/circle-progress.js"></script>
<script src="../default/js/pagespeed.min.js" type="text/javascript"></script>
<link href="../default/css/www.css" rel="stylesheet" />

<div class="container">
    <div class="row">

        <div class="col-sm-12">
            <div id="overview">

                <br />
                <br />

                <div id="scoreBoard" class="row">
                    <div class="col-md-4 screenBox">
                        <div id="screenshot">
                            <div id="screenshotData">
                                <div class="loader">
                                    <div class="side"></div>
                                    <div class="side"></div>
                                    <div class="side"></div>
                                    <div class="side"></div>
                                    <div class="side"></div>
                                    <div class="side"></div>
                                    <div class="side"></div>
                                    <div class="side"></div>
                                </div>
<!--                                <div class="loaderLabel">Loading...</div>-->
                            </div>
                            <div class="computer"></div>
                        </div>
                    </div>
                    <div class="col-md-5 levelBox">
                        <div>
                            <a href="http://mantralabsglobal.com" target="_blank" rel="nofollow" class="mainLink"><%= url %></a>
                        </div>
                        <div class="timeBox"> <%= moment().format('Do MMMM, YYYY'); %> </div>
                        <div class="progressBox">
                            <span class="scoreProgress-label passedBox">Passed</span>
                            <div class="scoreProgress scoreProgress-xs scoreProgress-success">
                                <div id="passScore" aria-valuemax="100" aria-valuenow="0" aria-valuemin="0" role="progressbar" class="scoreProgress-bar">
                                    <span class="scoreProgress-value">0%</span>
                                </div>
                            </div>
                        </div>

                        <div class="progressBox">
                            <span class="scoreProgress-label improveBox">To Improve</span>
                            <div class="scoreProgress scoreProgress-xs scoreProgress-warning">
                                <div id="improveScore" aria-valuemax="100" aria-valuenow="0" aria-valuemin="0" role="progressbar" class="scoreProgress-bar">
                                    <span class="scoreProgress-value">0%</span>
                                </div>
                            </div>
                        </div>

                        <div class="progressBox">
                            <span class="scoreProgress-label errorBox">Errors</span>
                            <div class="scoreProgress scoreProgress-xs scoreProgress-danger">
                                <div id="errorScore" aria-valuemax="100" aria-valuenow="0" aria-valuemin="0" role="progressbar" class="scoreProgress-bar">
                                    <span class="scoreProgress-value">0%</span>
                                </div>
                            </div>
                        </div>

                        <br />



                    </div>
                    <div class="col-md-2 circleBox">
                        <div class="second circle" data-size="130" data-thickness="5"><canvas width="130" height="130"></canvas>
                            <strong id="overallscore">0<i class="newI">Score</i></strong>
                        </div>
                    </div>

                </div>


            </div>

            <div class="clearfix"></div>

            <div id="seo">
                <div class="clearSep"></div>
                <h2 class="seoBox-title">
                    Search Engine Optimization
                </h2>

                <% let i = 1; value.scoreArray.forEach(function(score){ %>

                    <div class="seoBox" onclick="javascript:showSuggestion('seoBox<%= i %>');">
                        <div class="headBox clearfix">
                            <h4 class="titleStr"><%= score.name %> ( <%= score.score %> / <%= score.totalScore %> )</h4>
                            <h4 class="solveMsg" data-original-title="Easy to solve" data-toggle="tooltip" data-placement="top">
                                <i class="fa fa-gear fa-xs solveMsgBlue"></i>
                                <i class="fa fa-gear fa-xs "></i>
                                <i class="fa fa-gear fa-xs "></i>
                            </h4>
                        </div>
                        <div class="contentBox" id="seoBox<%= i %>">
                            <div class="<% if(score.score > 0){ %> passedBox <% } else { %> errorBox  <% } %>">
                                <div class="msgBox bottom10">
                                    <%= score.value %>
                                    <br />

                                    <% for(let reason of score.reasonPositive) { %>
                                        <%= reason %> <br>
                                    <% }; %>
                                </div>
                                <% if(score.reasonNegative.length > 0){ %>
                                    <div class="seoBox<%= i %> suggestionBox">
                                        <% for(let reason of score.reasonNegative) { %>
                                            <%= reason %> <br>
                                        <% }; %>
                                    </div>
                                <% } %>
                            </div>
                        </div>
                        <div class="questionBox" data-original-title="More Information" data-toggle="tooltip" data-placement="top">
                            <i class="fa fa-question-circle grayColor"></i>
                        </div>
                    </div>
                <% i++; }); %>

            </div>

            <div class="text-center">
                <br /> &nbsp; <br />
                <h4 style="color: #989ea8;">Try New Site</h4>
                <form method="POST" action="/start">
                    <div class="input-group reviewBox">
                        <div class="input-container">
                            <input type="text" tabindex="1" placeholder="Website URL to review" id="url" name="url" class="form-control reviewIn"/>
                        </div>

                        <div class="input-group-btn">
                            <button tabindex="2" type="submit" name="generate" class="btn btn-info url-lg">
                                <span class="ready">Analyze</span>
                            </button>
                        </div>
                    </div>
                </form>
                <br />
            </div>


            <br />

        </div>

    </div>
</div>

<!-- App -->
<script src="../default/js/dbdomain.js" type="text/javascript"></script>
